#include <Arduino.h>
#include <driver/i2s.h>
#include <WiFi.h>
#include <HTTPClient.h>
#include <SPIFFS.h>

// ===== Cáº¤U HÃŒNH WIFI =====
const char* ssid = "YOUR_WIFI_SSID";          // Thay tÃªn WiFi
const char* password = "YOUR_WIFI_PASSWORD";   // Thay máº­t kháº©u WiFi

// ===== GOOGLE APPS SCRIPT URL =====
const char* scriptURL = "YOUR_GOOGLE_SCRIPT_URL";  // URL tá»« Google Script

// ===== CHÃ‚N Káº¾T Ná»I 2 MIC INMP441 =====
// MIC 1 (TrÃ¡i)
#define I2S_WS_1    15    // L/R hoáº·c WS
#define I2S_SD_1    32    // SD hoáº·c DOUT  
#define I2S_SCK_1   14    // SCK hoáº·c BCLK

// MIC 2 (Pháº£i)
#define I2S_WS_2    25    
#define I2S_SD_2    33    
#define I2S_SCK_2   26    

// LÆ°u Ã½: VDD -> 3.3V, GND -> GND cho cáº£ 2 mic

// ===== Cáº¤U HÃŒNH GHI Ã‚M =====
#define SAMPLE_RATE   16000   // 16kHz cho giá»ng nÃ³i
#define RECORD_TIME   5       // 5 giÃ¢y
#define I2S_PORT_1    I2S_NUM_0
#define I2S_PORT_2    I2S_NUM_1
#define BUFFER_SIZE   512

// Cáº¥u trÃºc WAV Header
typedef struct {
  char riff[4];
  uint32_t fileSize;
  char wave[4];
  char fmt[4];
  uint32_t fmtSize;
  uint16_t audioFormat;
  uint16_t channels;
  uint32_t sampleRate;
  uint32_t byteRate;
  uint16_t blockAlign;
  uint16_t bitsPerSample;
  char data[4];
  uint32_t dataSize;
} WAVHeader;

// ===== KHá»I Táº O I2S CHO Má»–I MIC =====
void setupI2S(i2s_port_t port, int ws, int sd, int sck) {
  i2s_config_t i2s_config = {
    .mode = (i2s_mode_t)(I2S_MODE_MASTER | I2S_MODE_RX),
    .sample_rate = SAMPLE_RATE,
    .bits_per_sample = I2S_BITS_PER_SAMPLE_32BIT,
    .channel_format = I2S_CHANNEL_FMT_ONLY_LEFT,
    .communication_format = I2S_COMM_FORMAT_I2S,
    .intr_alloc_flags = ESP_INTR_FLAG_LEVEL1,
    .dma_buf_count = 8,
    .dma_buf_len = BUFFER_SIZE,
    .use_apll = false,
    .tx_desc_auto_clear = false,
    .fixed_mclk = 0
  };

  i2s_pin_config_t pin_config = {
    .bck_io_num = sck,
    .ws_io_num = ws,
    .data_out_num = I2S_PIN_NO_CHANGE,
    .data_in_num = sd
  };

  i2s_driver_install(port, &i2s_config, 0, NULL);
  i2s_set_pin(port, &pin_config);
  i2s_zero_dma_buffer(port);
}

// ===== Táº O WAV HEADER =====
void createWAVHeader(WAVHeader* header, uint32_t dataSize) {
  memcpy(header->riff, "RIFF", 4);
  header->fileSize = dataSize + 36;
  memcpy(header->wave, "WAVE", 4);
  memcpy(header->fmt, "fmt ", 4);
  header->fmtSize = 16;
  header->audioFormat = 1;
  header->channels = 2;  // Stereo
  header->sampleRate = SAMPLE_RATE;
  header->bitsPerSample = 16;
  header->byteRate = SAMPLE_RATE * 2 * 2;
  header->blockAlign = 4;
  memcpy(header->data, "data", 4);
  header->dataSize = dataSize;
}

// ===== GHI Ã‚M Tá»ª 2 MIC =====
void recordAudio(const char* filename) {
  File file = SPIFFS.open(filename, FILE_WRITE);
  if (!file) {
    Serial.println("âŒ Lá»—i: KhÃ´ng thá»ƒ táº¡o file!");
    return;
  }

  // Dá»± trá»¯ chá»— cho header
  WAVHeader header;
  file.write((uint8_t*)&header, sizeof(WAVHeader));

  int32_t buffer1[BUFFER_SIZE];
  int32_t buffer2[BUFFER_SIZE];
  int16_t stereoBuffer[BUFFER_SIZE * 2];
  size_t bytesRead1, bytesRead2;
  uint32_t totalDataSize = 0;
  
  uint32_t totalSamples = SAMPLE_RATE * RECORD_TIME;
  uint32_t samplesRecorded = 0;

  Serial.println("ğŸ™ï¸  Báº®T Äáº¦U GHI Ã‚M 5 GIÃ‚Y...");
  Serial.println("â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”");

  unsigned long startTime = millis();

  while (samplesRecorded < totalSamples) {
    // Äá»c Ä‘á»“ng thá»i tá»« 2 mic
    i2s_read(I2S_PORT_1, buffer1, BUFFER_SIZE * 4, &bytesRead1, portMAX_DELAY);
    i2s_read(I2S_PORT_2, buffer2, BUFFER_SIZE * 4, &bytesRead2, portMAX_DELAY);
    
    size_t samplesRead = bytesRead1 / 4;
    
    // GhÃ©p 2 kÃªnh thÃ nh stereo (TrÃ¡i-Pháº£i)
    for (int i = 0; i < samplesRead; i++) {
      stereoBuffer[i * 2] = (buffer1[i] >> 16) & 0xFFFF;       // KÃªnh trÃ¡i (Mic 1)
      stereoBuffer[i * 2 + 1] = (buffer2[i] >> 16) & 0xFFFF;   // KÃªnh pháº£i (Mic 2)
    }
    
    size_t bytesToWrite = samplesRead * 4;
    file.write((uint8_t*)stereoBuffer, bytesToWrite);
    totalDataSize += bytesToWrite;
    samplesRecorded += samplesRead;
    
    // Hiá»ƒn thá»‹ progress bar
    int progress = (samplesRecorded * 100) / totalSamples;
    if (progress % 20 == 0 && samplesRecorded % (SAMPLE_RATE / 2) == 0) {
      Serial.printf("â–“");
    }
  }

  unsigned long elapsed = millis() - startTime;
  Serial.println();
  Serial.printf("âœ… HoÃ n táº¥t! Thá»i gian: %lu ms\n", elapsed);
  Serial.printf("ğŸ“Š KÃ­ch thÆ°á»›c: %d KB\n", (totalDataSize + sizeof(WAVHeader))/1024);

  // Ghi header vá»›i thÃ´ng tin chÃ­nh xÃ¡c
  file.seek(0);
  createWAVHeader(&header, totalDataSize);
  file.write((uint8_t*)&header, sizeof(WAVHeader));
  file.close();
}

// ===== MÃƒ HÃ“A BASE64 =====
String base64Encode(uint8_t* data, size_t len) {
  const char* base64Chars = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/";
  String encoded = "";
  encoded.reserve((len * 4) / 3 + 4);
  
  for (size_t i = 0; i < len; i += 3) {
    uint32_t b = (data[i] << 16) | ((i + 1 < len ? data[i + 1] : 0) << 8) | (i + 2 < len ? data[i + 2] : 0);
    encoded += base64Chars[(b >> 18) & 0x3F];
    encoded += base64Chars[(b >> 12) & 0x3F];
    encoded += (i + 1 < len) ? base64Chars[(b >> 6) & 0x3F] : '=';
    encoded += (i + 2 < len) ? base64Chars[b & 0x3F] : '=';
  }
  return encoded;
}

// ===== UPLOAD LÃŠN GOOGLE DRIVE =====
bool uploadToGoogleDrive(const char* filename) {
  File file = SPIFFS.open(filename, FILE_READ);
  if (!file) {
    Serial.println("âŒ KhÃ´ng thá»ƒ Ä‘á»c file!");
    return false;
  }

  Serial.println("â˜ï¸  ÄANG UPLOAD LÃŠN GOOGLE DRIVE...");
  
  size_t fileSize = file.size();
  Serial.printf("ğŸ“¦ KÃ­ch thÆ°á»›c file: %d bytes\n", fileSize);
  
  uint8_t* buffer = (uint8_t*)malloc(fileSize);
  if (!buffer) {
    Serial.println("âŒ KhÃ´ng Ä‘á»§ RAM!");
    file.close();
    return false;
  }
  
  file.read(buffer, fileSize);
  file.close();

  Serial.print("ğŸ”„ Äang mÃ£ hÃ³a... ");
  String encoded = "";
  size_t chunkSize = 3000;
  for (size_t i = 0; i < fileSize; i += chunkSize) {
    size_t len = min(chunkSize, fileSize - i);
    encoded += base64Encode(buffer + i, len);
    if (i % 15000 == 0) Serial.print(".");
  }
  free(buffer);
  Serial.println(" OK");

  // Táº¡o tÃªn file vá»›i timestamp
  String timestamp = String(millis());
  String jsonData = "{\"filename\":\"recording_" + timestamp + ".wav\",\"data\":\"" + encoded + "\"}";

  HTTPClient http;
  http.begin(scriptURL);
  http.addHeader("Content-Type", "application/json");
  http.setTimeout(30000);
  
  Serial.print("ğŸ“¤ Äang gá»­i... ");
  int httpCode = http.POST(jsonData);
  
  if (httpCode > 0) {
    String response = http.getString();
    Serial.println("OK");
    Serial.println("âœ… UPLOAD THÃ€NH CÃ”NG!");
    Serial.println("ğŸ”— Link file: " + response);
    http.end();
    return true;
  } else {
    Serial.println("THáº¤T Báº I");
    Serial.printf("âŒ MÃ£ lá»—i HTTP: %d\n", httpCode);
    Serial.printf("âŒ Chi tiáº¿t: %s\n", http.errorToString(httpCode).c_str());
    http.end();
    return false;
  }
}

// ===== SETUP =====
void setup() {
  Serial.begin(115200);
  delay(1000);
  
  Serial.println("\nâ•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—");
  Serial.println("â•‘  ESP32-S DUAL MIC RECORDER v1.0   â•‘");
  Serial.println("â•‘  2 x INMP441 â†’ Google Drive        â•‘");
  Serial.println("â•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n");
  
  // Khá»Ÿi táº¡o SPIFFS
  Serial.print("ğŸ’¾ Khá»Ÿi táº¡o SPIFFS... ");
  if (!SPIFFS.begin(true)) {
    Serial.println("THáº¤T Báº I!");
    while(1) delay(1000);
  }
  Serial.println("OK");
  
  // Káº¿t ná»‘i WiFi
  Serial.printf("ğŸ“¶ Äang káº¿t ná»‘i WiFi: %s\n", ssid);
  WiFi.begin(ssid, password);
  
  int attempts = 0;
  while (WiFi.status() != WL_CONNECTED && attempts < 30) {
    delay(500);
    Serial.print(".");
    attempts++;
  }
  Serial.println();
  
  if (WiFi.status() != WL_CONNECTED) {
    Serial.println("âŒ KHÃ”NG THá»‚ Káº¾T Ná»I WIFI!");
    Serial.println("âš ï¸  Kiá»ƒm tra láº¡i SSID vÃ  Password");
    while(1) delay(1000);
  }
  
  Serial.println("âœ… ÄÃ£ káº¿t ná»‘i WiFi!");
  Serial.print("ğŸŒ Äá»‹a chá»‰ IP: ");
  Serial.println(WiFi.localIP());
  Serial.print("ğŸ“¶ CÆ°á»ng Ä‘á»™ tÃ­n hiá»‡u: ");
  Serial.print(WiFi.RSSI());
  Serial.println(" dBm\n");
  
  // Khá»Ÿi táº¡o I2S cho 2 mic
  Serial.println("ğŸ›ï¸  Äang khá»Ÿi táº¡o I2S...");
  Serial.println("   â†’ MIC 1 (TrÃ¡i): WS=15, SD=32, SCK=14");
  setupI2S(I2S_PORT_1, I2S_WS_1, I2S_SD_1, I2S_SCK_1);
  Serial.println("   â†’ MIC 2 (Pháº£i): WS=25, SD=33, SCK=26");
  setupI2S(I2S_PORT_2, I2S_WS_2, I2S_SD_2, I2S_SCK_2);
  Serial.println("âœ… I2S sáºµn sÃ ng!\n");
  
  delay(1000);
  Serial.println("ğŸš€ Há»† THá»NG Sáº´N SÃ€NG!\n");
  Serial.println("â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n");
}

// ===== LOOP =====
void loop() {
  const char* audioFile = "/recording.wav";
  
  Serial.println("ğŸ¬ Báº®T Äáº¦U CHU Ká»² Má»šI");
  Serial.println("â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•");
  
  // Ghi Ã¢m 5 giÃ¢y
  recordAudio(audioFile);
  
  Serial.println();
  
  // Upload lÃªn Google Drive
  if (uploadToGoogleDrive(audioFile)) {
    // XÃ³a file sau khi upload thÃ nh cÃ´ng
    SPIFFS.remove(audioFile);
    Serial.println("ğŸ—‘ï¸  ÄÃ£ xÃ³a file táº¡m");
  } else {
    Serial.println("âš ï¸  File táº¡m váº«n cÃ²n trong SPIFFS");
  }
  
  // Chá» 10 giÃ¢y trÆ°á»›c chu ká»³ tiáº¿p theo
  Serial.println("\nâ³ Chá» 10 giÃ¢y trÆ°á»›c khi ghi Ã¢m tiáº¿p...");
  Serial.println("â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n");
  delay(10000);
}

